<AnchorPane prefHeight="378.0" prefWidth="600.0" style="-fx-background-color: #befffc;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stockfinal.AdminApprovalController">
    <children>
        <TableView fx:id="transactionsTable" layoutX="14.0" layoutY="21.0" prefHeight="300.0" prefWidth="572.0">
            <columns>
                <TableColumn fx:id="typeColumn" text="Type" />
                <TableColumn fx:id="amountColumn" text="Amount" />
                <TableColumn fx:id="statusColumn" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="approveButton" layoutX="14.0" layoutY="335.0" mnemonicParsing="false" onAction="#handleApprove" style="-fx-background-color: #025059;" text="Approve" textFill="WHITE" />
        <Button fx:id="rejectButton" layoutX="100.0" layoutY="335.0" mnemonicParsing="false" onAction="#handleReject" style="-fx-background-color: #025059;" text="Reject" textFill="WHITE" />
    </children>
</AnchorPane>
